#include<stdio.h>
void threedarray();
void fivedarray();
void char3darray();
int main()
{
	int a[3][3]={{1, 2, 3}, {4, 5, 6},{7, 8, 9}};
	int i=0,j=0;

	for(i=0;i<3;i++)
	{		
		for(j=0;j<3;j++)
		{
			if(i==j)
			{printf("%d\t", a[i][j]);}
		}
		printf("\n");
	}
	printf("\n\n\n");
	threedarray();
	printf("\n\n\n");
	fivedarray();
	printf("\n\n\n");
	char3darray();
	printf("\n\n\n");
	return 0;
}

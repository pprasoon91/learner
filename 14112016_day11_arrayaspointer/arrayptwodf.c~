#include "arrayph.h"
void arraypchdf()
{
	char *a = malloc(5 * sizeof(char));
	int i, j;
	char k;
	printf("\nenter the characters, upto 5 characters can be entered\n");
	for(i=0;i<5;i++)
	{
		printf("enter %d character",i);
		scanf("%c", &k);
		*(a+i)=k;	
	}
	print("\nthe entered characters are\n");
	for(j=0;j<5;j++)
	{
		printf("\n%d\n\n",*(a+j));		
	}
}
